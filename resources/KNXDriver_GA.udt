TYPE "KNXDriver_GA"
VERSION : 0.1
   STRUCT
      GA : DWord := 0000000;   // GroupAddress 00/00/000 without slash
      "Type" : Int := 1;   // DPT Type
      "READ_ONLY" : Bool;   // True to only read from bus
      WRITE_ONLY : Bool;   // True to only send to bus
      val_bool : Bool;   // Value for DPT1
      val_int : Int;   // Value for DPT5
      val_real : Real;   // Value for DPT9
   END_STRUCT;

END_TYPE

